{
  "is_capcut_env": true,
  "draft_domain": "https://www.install-ai-guider.top",
  "port": 9001,
  "preview_router": "/draft/downloader",
  "is_upload_draft": false,

  "api_keys": [
    "your-api-key-1",
    "your-api-key-2"
  ],
  "route_policies": {
    "default": { "protected": true },
    "/get_intro_animation_types": { "protected": false }
  },
  "rate_limit": {
    "enabled": true,
    "requests": 60,
    "per_seconds": 60
  },

  "oss_config": {
    "bucket_name": "your-bucket-name",
    "access_key_id": "your-access-key-id",
    "access_key_secret": "your-access-key-secret",
    "endpoint": "https://your-endpoint.aliyuncs.com"
  },
  "mp4_oss_config": {
    "bucket_name": "your-mp4-bucket-name",
    "access_key_id": "your-access-key-id",
    "access_key_secret": "your-access-key-secret",
    "region": "your-region",
    "endpoint": "http://your-custom-domain"
  }
}